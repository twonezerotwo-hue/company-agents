  "components":  {
    "schemas": {
      "LoginRequest": {
        "type":  "object",
        "properties":  {
          "username": {
            "type": "string",
            "minLength": 3,
            "maxLength": 50
          },
          "password": {
            "type": "string",
            "minLength": 8
          }
        },
        "required": ["username", "password"],
        "title": "LoginRequest"
      },
      "TokenResponse": {
        "type": "object",
        "properties": {
          "access_token": {
            "type": "string"
          },
          "token_type": {
            "type":  "string",
            "enum": ["bearer"]
          }
        },
        "required":  ["access_token", "token_type"],
        "title": "TokenResponse"
      },
      "HoldingCreate": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "minLength":  1,
            "maxLength": 100,
            "title": "Name"
          },
          "description": {
            "type": "string",
            "maxLength": 500,
            "nullable": true,
            "title":  "Description"
          }
        },
        "required": ["name"],
        "title": "HoldingCreate"
      },
      "HoldingResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "title": "Id"
          },
          "name": {
            "type": "string",
            "title": "Name"
          },
          "description": {
            "type": "string",
            "nullable": true,
            "title":  "Description"
          },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time",
            "title": "Updated At"
          }
        },
        "required": ["id", "name", "created_at", "updated_at"],
        "title": "HoldingResponse"
      },
      "HoldingWithCompanies": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name":  {
            "type": "string"
          },
          "description":  {
            "type": "string",
            "nullable": true
          },
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time"
          },
          "companies": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CompanyResponse"
            },
            "default": []
          }
        },
        "required": ["id", "name"],
        "title": "HoldingWithCompanies"
      },
      "CompanyCreate": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "minLength": 1,
            "maxLength": 100,
            "title": "Name"
          },
          "description":  {
            "type": "string",
            "maxLength": 500,
            "nullable": true,
            "title": "Description"
          },
          "holding_id": {
            "type": "integer",
            "minimum": 1,
            "title": "Holding Id"
          }
        },
        "required": ["name", "holding_id"],
        "title": "CompanyCreate"
      },
      "CompanyResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name":  {
            "type": "string"
          },
          "description":  {
            "type": "string",
            "nullable": true
          },
          "holding_id": {
            "type": "integer"
          },
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time"
          }
        },
        "required": ["id", "name", "holding_id"],
        "title": "CompanyResponse"
      },
      "CompanyWithDepartments": {
        "type": "object",
        "properties": {
          "id": {
            "type":  "integer"
          },
          "name": {
            "type": "string"
